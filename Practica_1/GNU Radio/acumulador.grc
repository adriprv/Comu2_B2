- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\n\n\nclass blk(gr.sync_block):\
      \  \n\n\n    def __init__(self,):  \n        \"\"\"bloque acumulador simplemente\
      \ toma la se\xF1al entrante y entrega acmulada\"\"\"\n        gr.sync_block.__init__(\n\
      \            self,\n            name='Acumulador',   \n            in_sig=[np.float32],\n\
      \            out_sig=[np.float32]\n                    \n        )\n       \
      \ \n        self.acum_anterior=0\n        \n\n    def work(self, input_items,\
      \ output_items):\n  \t\n        x= input_items[0]\n        y= output_items[0]\n\
      \        N=len(x)\n        y[:]=self.acum_anterior+np.cumsum(x)\n        self.acum_anterior=y[N-1]\n\
      \n        \n    \n        return len(x)\n"
    affinity: ''
    alias: ''
    comment: ''
    maxoutbuf: '0'
    minoutbuf: '0'
  states:
    _io_cache: "('Acumulador', 'blk', [], [('0', 'float', 1)], [('0', 'float', 1)],\
      \ 'bloque acumulador simplemente toma la se\xF1al entrante y entrega acmulada',\
      \ [])"
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [432, 316.0]
    rotation: 0
